https://leetcode.com/problems/missing-number/submissions/


class Solution {
    public int missingNumber(int[] nums) {
        int n=nums.length;
        int count=0;
        for(int i=0;i<n;i++){
            count= count+nums[i];
            
        }
        int sum=0;
        for(int i=0;i<n;i++){
        sum= n*(n+1)/2 ;
        }
        return sum-count;
        
    }
}
